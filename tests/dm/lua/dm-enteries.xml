    <action>0x100</action>   
    <param>
	local str 

	-- String format
    	vscp.log('Before command - string (default)')
	str = vscp.readvariable('vscp.version.str')
        vscp.log( str )

	-- XML format
    	vscp.log('Before command - XML')
	str = vscp.readvariable('vscp.version.str',1)
        vscp.log( str )

	-- JSON format
    	vscp.log('Before command - JSON')
	str = vscp.readvariable('vscp.version.str',2)
        vscp.log( str )

	-- Just value
    	vscp.log('Before command - just value')
	str = vscp.readvariable('vscp.version.str',3)
        vscp.log( str )

        -- Just note
    	vscp.log('Before command - just note')
	str = vscp.readvariable('vscp.version.str',4)
        vscp.log( str )

	vscp.log('THE END')
    </param>


    <allowed_from>*</allowed_from>
    <allowed_to>*</allowed_to>
    <allowed_weekdays>*</allowed_weekdays>
